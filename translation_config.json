{
  "translation_routes": {
    "zh": {
      "en": {
        "path": ["zh-en"],
        "models": ["Helsinki-NLP/opus-mt-zh-en"]
      },
      "el": {
        "path": ["zh-en", "en-el"],
        "models": [
          "Helsinki-NLP/opus-mt-zh-en",
          "Helsinki-NLP/opus-mt-en-el"
        ]
      }
    },
    "en": {
      "zh": {
        "path": ["en-zh"],
        "models": ["Helsinki-NLP/opus-mt-en-zh"]
      },
      "el": {
        "path": ["en-el"],
        "models": ["Helsinki-NLP/opus-mt-en-el"]
      }
    },
    "el": {
      "en": {
        "path": ["el-en"],
        "models": ["Helsinki-NLP/opus-mt-tc-big-el-en"]
      },
      "zh": {
        "path": ["el-en", "en-zh"],
        "models": [
          "Helsinki-NLP/opus-mt-tc-big-el-en",
          "Helsinki-NLP/opus-mt-en-zh"
        ]
      }
    }
  },
  "language_names": {
    "zh": "Chinese",
    "en": "English",
    "el": "Greek"
  },
  "default_intermediate": "en",
  "cache_models": true,
  "device": "auto"
}
